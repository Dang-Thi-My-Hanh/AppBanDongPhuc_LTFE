                       currentPrice = currentPrice.replace('đ', '').replace(/\./g, '').replace(/^\s+|\s+$/gm, '');
                       price = currentPrice ? parseInt(currentPrice) : 0;
                   }else{
                       price = 120000;
                   }
                   if(marketing_currency != 'VND'){
                       price = price/marketing_exchange_rate;
                   }
                   return price;
               } catch (e) {}
            }

            $('body').on('click', '.event-email, .event-hotline, .event-zalo', function (e) {
                const conversation_id = $(this).attr('data-tracking-id');
                if(conversation_id!==''){
                  e.preventDefault();
                  gtag_report_conversion($(this).attr('href'), conversation_id, getPriceCurent());
                }
            });
        });

        function sortAllHotlines() {
            const containers = document.querySelectorAll('.hotline-invert-by-day');
            const today = new Date();
            const dayOfMonth = today.getDate();

            containers.forEach(container => {
                const groups = container.querySelectorAll('.hotline-item-day');
                const numGroups = groups.length;
                const indexToMove = (dayOfMonth - 1) % numGroups;

                const groupToMove = groups[indexToMove];
                if (groupToMove) {
                    container.prepend(groupToMove);
                }
            });
        }
